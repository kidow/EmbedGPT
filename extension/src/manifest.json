{
  "manifest_version": 3,
  "name": "EmbedGPT - Embed your ChatGPT content to community",
  "version": "1.0.0",
  "description": "EmbedGPT lets you embed ChatGPT content on Twitter, Reddit, etc.",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "16x16.png",
      "32": "32x32.png"
    },
    "default_title": "Share Content"
  },
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/chat/*"],
      "js": ["contentScript.bundle.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": ["https://embedgpt.vercel.app/*"]
}
