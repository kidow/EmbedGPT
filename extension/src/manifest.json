{
  "manifest_version": 3,
  "name": "__MSG_name__",
  "version": "1.2.0",
  "description": "__MSG_description__",
  "background": {
    "service_worker": "background.bundle.js"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_icon": {
      "16": "16x16.png",
      "32": "32x32.png"
    },
    "default_title": "Share Content"
  },
  "icons": {
    "16": "16x16.png",
    "32": "32x32.png"
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/chat/*"],
      "js": ["contentScript.bundle.js"],
      "run_at": "document_end"
    }
  ],
  "default_locale": "ko"
}
